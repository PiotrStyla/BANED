{
  "version": 2,
  "builds": [
    {
      "src": "api_vercel.py",
      "use": "@vercel/python"
    },
    {
      "src": "public/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "/public/index.html"
    },
    {
      "src": "/predict",
      "dest": "/api_vercel.py"
    },
    {
      "src": "/api/(.*)",
      "dest": "/api_vercel.py"
    },
    {
      "src": "/health",
      "dest": "/api_vercel.py"
    }
  ]
}
