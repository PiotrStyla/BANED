{
  "version": 2,
  "builds": [
    {
      "src": "api_vercel.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "/api_vercel.py"
    },
    {
      "src": "/predict",
      "dest": "/api_vercel.py"
    },
    {
      "src": "/web",
      "dest": "/static/double_power.html"
    },
    {
      "src": "/(.*)",
      "dest": "/api_vercel.py"
    }
  ]
}
