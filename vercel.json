{
  "version": 2,
  "functions": {
    "api/predict.js": {
      "runtime": "nodejs18.x"
    }
  },
  "routes": [
    {
      "src": "/api",
      "dest": "/api/predict.js"
    },
    {
      "src": "/api/(.*)",
      "dest": "/api/predict.js"
    },
    {
      "src": "/(.*)",
      "dest": "docs/$1"
    }
  ]
}
