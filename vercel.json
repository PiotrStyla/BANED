{
  "version": 2,
  "builds": [
    {
      "src": "api.py",
      "use": "@vercel/python"
    },
    {
      "src": "docs/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "api.py"
    },
    {
      "src": "/(.*)",
      "dest": "docs/$1"
    }
  ],
  "env": {
    "PYTHON_VERSION": "3.11"
  }
}
